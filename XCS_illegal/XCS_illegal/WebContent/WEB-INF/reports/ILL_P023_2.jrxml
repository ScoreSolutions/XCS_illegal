	<queryString>
		<![CDATA[select ar.id application_reward_request_id, ar.request_no, ed.offname,
to_char(ar.request_date,'fmdd') request_date,
to_char(ar.request_date, 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  request_month,
to_char(ar.request_date, 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  request_year,
tir.title_name || str.first_name || ' ' || str.last_name request_name, psr.posname ||  str.staff_level posname_request,
ed.offname offname_request,pke_arrest.getArrestLawbreaker(aa.track_no) lawbreaker_name, lg.legislation_name,
pke_arrest.getArrestIndictment(aa.track_no) guilt_base,
aa.scene_name || ' ' || aa.scene_no || case when aa.scene_soi is null then '' else ' ???' || aa.scene_soi end ||
case when aa.scene_road is null then ' ' else ' ???' || aa.scene_road end ||
case when aa.scene_moo is null then ' ' else ' ??????? ' || aa.scene_moo || ' '   end scene_addr,
sd.subdistrict_name, d.district_name, p.province_name,
pke_arrest.getRewardReqTeam(ar.id,to_number(pke_master.getConfigValue('arrest_co'))) arrest_team,
pke_arrest.getRewardReqTeam(ar.id,to_number(pke_master.getConfigValue('commander'))) command_team,
pke_arrest.getsceneaddress(aa.track_no) scene_address,
to_char(aa.occurrence_date,'fmdd') occurrence_date,
to_char(aa.occurrence_date,'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') occurrence_month,
to_char(aa.occurrence_date,'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') occurrence_year,
nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) case_fine,
nvl(cc.reward_money,0) reward_money,cc.compare_case_no,
to_char(cc.pay_date,'fmdd') pay_date,
to_char(cc.pay_date,'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') pay_month,
to_char(cc.pay_date,'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') pay_year,
to_char(cc.compare_case_date,'fmdd') compare_date,
to_char(cc.compare_case_date,'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') compare_month,
to_char(cc.compare_case_date,'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') compare_year,
tia.title_name || sta.first_name || ' ' || sta.last_name accuser_name, psa.posname ||  sta.staff_level accuser_posname,
nvl(aa.accuser_offname, eda.offname) accuser_offname,
tis.title_name || sts.first_name || ' ' || sts.last_name sue_staff_name,pss.posname ||  sts.staff_level sue_posname,
idc.case_law_id, idc.penalty_case_law_id, art.lawyer_opinion,art.law_director_opinion, art.law_director_position,
art.command,art.arrest_opinion
from application_reward_request ar
inner join ed_office ed on ed.offcode=ar.offcode
inner join compare_case cc on cc.id=ar.compare_case_id
inner join application_arrest aa on aa.track_no=cc.track_no
left join indictment idc on idc.indictment_id=cc.indictment_id
left join staff str on str.idcard_no=ar.staff_id_request
left join title tir on tir.title_code=str.title_code
left join position psr on psr.poscode=trim(ar.poscode_request)
left join staff sta on sta.idcard_no=aa.accuser_idcardno
left join title tia on tia.title_code=sta.title_code
left join ed_office eda on eda.offcode=sta.offcode
left join position psa on psa.poscode=aa.accuser_poscode
inner join legislation lg on lg.id=aa.legislation_id
left join staff sts on sts.idcard_no=cc.sue_staff_idcardno
left join title tis on tis.title_code=sts.title_code
left join position pss on pss.poscode=trim(cc.sue_poscode)
inner join application_reward_req_tobacco art on ar.id=art.application_reward_request_id
left join subdistrict sd on sd.subdistrict_code=aa.scene_subdistrict
left join district d on d.district_code=aa.scene_district
left join province p on p.province_code=aa.scene_province
where ar.request_no = $P{getTrackNo}]]>
	</queryString>
