	<parameter name="vOffcode2" class="java.lang.String"/>
	<parameter name="vOffcode3" class="java.lang.String"/>
	<parameter name="getUserID" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select aa.*,
nvl((select short_name from ed_office where offcode = nvl($P{vOffcode3},nvl($P{vOffcode2},$P{vOffcode1}))),'รวมทั่วประเทศ') offname,
to_char(sysdate, 'fmdd month yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') untildate,
to_char(sysdate, 'hh24:mi', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') systimedate,
nvl((select short_name from ed_office where offcode = nvl($P{vOffcode3},nvl($P{vOffcode2},$P{vOffcode1}))),'รวมทั่วประเทศ') offname,
to_char(sysdate, 'fmdd month yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') untildate,
to_char(sysdate, 'hh24:mi', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') systimedate,
to_char(to_date(($P{vYear}-543)||'-'||substr($P{vMonthFrom},5,6)||'-01','yyyy-mm-dd'), 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') as Month_From ,
to_char(to_date(($P{vYear}-543)||'-'||substr($P{vMonthTo},5,6)||'-01','yyyy-mm-dd'), 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') as Month_To ,
to_char(to_date(substr($P{vMonthFrom},1,4)||'-'||substr($P{vMonthFrom},5,6)||'-01','yyyy-mm-dd'), 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') as Year_Name,

        (select ti.title_name || st.first_name || ' ' || st.last_name staff_name
                          from staff st
                          left join title ti on ti.title_code=st.title_code
                          where st.idcard_no=$P{getUserID}) user_name
from
(

select  1 as Grp_Id, '1. คดีที่จับโดย' as List_Detail,
             null  as Case_Oil,
             null as Case_Drink,
             null  as Case_Air,
             null  as Case_Light,
             null as Case_Crystal,
             null  as Case_Moter,
             null  as Case_Boat,
             null  as Case_Perfume,
             null  as Case_Carpet,
             null  as Case_Racecourse,
             null  as Case_Golf,
             null  as Case_Motorcycle,
             null  as Case_Battery,
             null  as Case_Facilities,
             null  as Case_Nightclub
from dual
union all
select   2 as Grp_Id,  '  1.1 เจ้าพนักงานสรรพสามิต (ราย)'  as List_Detail,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำมัน%' Then aa.id end)  as Case_Oil,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องดื่ม%' Then aa.id end)  as Case_Drink,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องปรับอากาศ%' Then aa.id end)  as Case_Air,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'โคมไฟฟ้า%' Then aa.id end)  as Case_Light,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'แก้วคริสตัล%' Then aa.id end)  as Case_Crystal,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'รถยนต์%' Then aa.id end)  as Case_Moter,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'เรือยอร์ช%' Then aa.id end)  as Case_Boat,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำหอม%' Then aa.id end)  as Case_Perfume,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'พรม%' Then aa.id end)  as Case_Carpet,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สนามม้า%' Then aa.id end)  as Case_Racecourse,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สนามกอล์ฟ%' Then aa.id end)  as Case_Golf,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'รถจักรยานยนต์%' Then aa.id end)  as Case_Motorcycle,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'แบตเตอรี่%' Then aa.id end)  as Case_Battery,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการอาบอบนวด%' Then aa.id end)  as Case_Facilities,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการไนท์คลับ%' Then aa.id end)  as Case_Nightclub
from  application_arrest  aa
inner join book_impeachment bi
       on aa.track_no=bi.track_no  and aa.legislation_id = 4 and aa.accuser_type <> '5'
inner join application_arrest_exhibit aae
       on aa.track_no=aae.track_no
left join duty_table dt
       on aae.duty_code = dt.duty_code
where to_char(bi.case_date,'yyyymm') between $P{vMonthFrom} and $P{vMonthTo}
   and  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
   and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
   and aa.offcode=nvl($P{vOffcode3},aa.offcode)
union all
select   3 as Grp_Id, '  1.2 เจ้าพนักงานอื่นๆ (ราย)'  as List_Detail,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำมัน%' Then aa.id end)  as Case_Oil,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องดื่ม%' Then aa.id end)  as Case_Drink,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องปรับอากาศ%' Then aa.id end)  as Case_Air,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'โคมไฟฟ้า%' Then aa.id end)  as Case_Light,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'แก้วคริสตัล%' Then aa.id end)  as Case_Crystal,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'รถยนต์%' Then aa.id end)  as Case_Moter,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'เรือยอร์ช%' Then aa.id end)  as Case_Boat,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำหอม%' Then aa.id end)  as Case_Perfume,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'พรม%' Then aa.id end)  as Case_Carpet,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สนามม้า%' Then aa.id end)  as Case_Racecourse,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สนามกอล์ฟ%' Then aa.id end)  as Case_Golf,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'รถจักรยานยนต์%' Then aa.id end)  as Case_Motorcycle,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'แบตเตอรี่%' Then aa.id end)  as Case_Battery,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการอาบอบนวด%' Then aa.id end)  as Case_Facilities,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการไนท์คลับ%' Then aa.id end)  as Case_Nightclub
from  application_arrest  aa
inner join book_impeachment bi
       on aa.track_no=bi.track_no  and aa.legislation_id = 4 and aa.accuser_type = '5'
inner join application_arrest_exhibit aae
       on aa.track_no=aae.track_no
left join duty_table dt
       on aae.duty_code = dt.duty_code
where to_char(bi.case_date,'yyyymm') between $P{vMonthFrom} and $P{vMonthTo}
   and  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
   and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
   and aa.offcode=nvl($P{vOffcode3},aa.offcode)
union all
select   4 as Grp_Id, '2. คดีส่งฟ้องศาล (รายงาน)'  as List_Detail,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำมัน%' Then aa.id end)  as Case_Oil,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องดื่ม%' Then aa.id end)  as Case_Drink,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องปรับอากาศ%' Then aa.id end)  as Case_Air,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'โคมไฟฟ้า%' Then aa.id end)  as Case_Light,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'แก้วคริสตัล%' Then aa.id end)  as Case_Crystal,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'รถยนต์%' Then aa.id end)  as Case_Moter,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'เรือยอร์ช%' Then aa.id end)  as Case_Boat,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำหอม%' Then aa.id end)  as Case_Perfume,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'พรม%' Then aa.id end)  as Case_Carpet,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สนามม้า%' Then aa.id end)  as Case_Racecourse,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สนามกอล์ฟ%' Then aa.id end)  as Case_Golf,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'รถจักรยานยนต์%' Then aa.id end)  as Case_Motorcycle,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'แบตเตอรี่%' Then aa.id end)  as Case_Battery,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการอาบอบนวด%' Then aa.id end)  as Case_Facilities,
             count(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการไนท์คลับ%' Then aa.id end)  as Case_Nightclub
from  application_arrest  aa
inner join book_impeachment bi
      on aa.track_no=bi.track_no  and aa.legislation_id = 4 and aa.have_culprit ='Y'  and bi.case_quality = '2'
inner join application_arrest_exhibit aae
       on aa.track_no=aae.track_no
left join duty_table dt
       on aae.duty_code = dt.duty_code
where to_char(bi.case_date,'yyyymm') between $P{vMonthFrom} and $P{vMonthTo}
   and  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
   and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
   and aa.offcode=nvl($P{vOffcode3},aa.offcode)
union all
select  5 as Grp_Id,  '3. ผลคดี' as List_Detail,
             null  as Case_Oil,
             null as Case_Drink,
             null  as Case_Air,
             null  as Case_Light,
             null as Case_Crystal,
             null  as Case_Moter,
             null  as Case_Boat,
             null  as Case_Perfume,
             null  as Case_Carpet,
             null  as Case_Racecourse,
             null  as Case_Golf,
             null  as Case_Motorcycle,
             null  as Case_Battery,
             null  as Case_Facilities,
             null  as Case_Nightclub
from dual
union all
select  6 as Grp_Id, '  ศาลปรับ (บาท)'   as List_Detail,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำมัน%' Then bi.case_fine end)  as Case_Oil,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องดื่ม%' Then bi.case_fine end)  as Case_Drink,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องปรับอากาศ%' Then bi.case_fine end)  as Case_Air,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'โคมไฟฟ้า%' Then bi.case_fine end)  as Case_Light,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'แก้วคริสตัล%' Then bi.case_fine end)  as Case_Crystal,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'รถยนต์%' Then bi.case_fine end)  as Case_Moter,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'เรือยอร์ช%' Then bi.case_fine end)  as Case_Boat,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำหอม%' Then bi.case_fine end)  as Case_Perfume,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'พรม%' Then bi.case_fine end)  as Case_Carpet,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'สนามม้า%' Then bi.case_fine end)  as Case_Racecourse,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'สนามกอล์ฟ%' Then bi.case_fine end)  as Case_Golf,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'รถจักรยานยนต์%' Then bi.case_fine end)  as Case_Motorcycle,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'แบตเตอรี่%' Then bi.case_fine end)  as Case_Battery,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการอาบอบนวด%' Then bi.case_fine end)  as Case_Facilities,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการไนท์คลับ%' Then bi.case_fine end)  as Case_Nightclub
from  application_arrest  aa
inner join book_impeachment bi
      on aa.track_no=bi.track_no  and aa.legislation_id = 4 and aa.have_culprit ='Y'  and bi.case_quality = '2'
inner join  application_arrest_exhibit aae
       on aa.track_no=aae.track_no
left join duty_table dt
       on aae.duty_code = dt.duty_code
where to_char(bi.case_last_date,'yyyymm') between $P{vMonthFrom} and $P{vMonthTo}
   and  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
   and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
   and aa.offcode=nvl($P{vOffcode3},aa.offcode)
union all
select  7 as Grp_Id, '  เปรียบเทียบปรับ (บาท)'  as List_Detail,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำมัน%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Oil,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องดื่ม%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Drink,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องปรับอากาศ%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Air,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'โคมไฟฟ้า%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end) as Case_Light,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'แก้วคริสตัล%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Crystal,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'รถยนต์%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Moter,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'เรือยอร์ช%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end) as Case_Boat,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำหอม%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Perfume,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'พรม%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Carpet,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'สนามม้า%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Racecourse,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'สนามกอล์ฟ%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Golf,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'รถจักรยานยนต์%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Motorcycle,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'แบตเตอรี่%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Battery,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการอาบอบนวด%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Facilities,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'สถานบริการไนท์คลับ%' Then nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) end)  as Case_Nightclub
from  application_arrest  aa
inner join book_impeachment bi
      on aa.track_no=bi.track_no  and aa.legislation_id = 4
inner join application_arrest_exhibit aae
       on aa.track_no=aae.track_no
inner join compare_case cc
      on cc.track_no=aa.track_no
left join duty_table dt
       on aae.duty_code = dt.duty_code
where to_char(cc.pay_date,'yyyymm') between $P{vMonthFrom} and $P{vMonthTo}
   and  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
   and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
   and aa.offcode=nvl($P{vOffcode3},aa.offcode)
union all
select    8 as Grp_Id, '  เงินสินบน (บาท)'  as List_Detail,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำมัน%' Then nvl(cc.birbe_money,0)  end)  as Case_Oil,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องดื่ม%' Then nvl(cc.birbe_money,0)  end)  as Case_Drink,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'เครื่องปรับอากาศ%' Then nvl(cc.birbe_money,0)  end)  as Case_Air,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'โคมไฟฟ้า%' Then nvl(cc.birbe_money,0)  end)  as Case_Light,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'แก้วคริสตัล%' Then nvl(cc.birbe_money,0)  end)  as Case_Crystal,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'รถยนต์%' Then nvl(cc.birbe_money,0)  end)  as Case_Moter,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'เรือยอร์ช%' Then nvl(cc.birbe_money,0)  end) as Case_Boat,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'น้ำหอม%' Then nvl(cc.birbe_money,0)  end)  as Case_Perfume,
             sum(case when nvl(aae.product_name,dt.duty_name) Like 'พรม%' Then nvl(cc.birbe_money,0)  end)  as Case_Carpet,
