    to_char(sysdate, 'dd month yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') untildate,
    to_char(sysdate, 'hh24:mi', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') systimedate,
		(select ti.title_name || st.first_name || ' ' || st.last_name staff_name
            	          from staff st
                          left join title ti on ti.title_code=st.title_code
                          where st.idcard_no='') user_name,
nvl(a.compare_case1 ,0)- nvl(a.compare_case3,0) diff_case1, nvl(a.compare_fine1,0) - nvl(a.compare_fine3,0) diff_fine1,
case when nvl(a.compare_case3,0) = 0 and nvl(a.compare_case1,0)=0 then 0
else
    case nvl(a.compare_case3,0) when 0 then 100
    else
        ((nvl(a.compare_case1,0) - a.compare_case3)*100)/a.compare_case3
    end
end diff_case_percent1,
case when nvl(a.compare_fine3,0) = 0 and nvl(a.compare_fine1,0)=0 then 0
else
    case nvl(a.compare_fine3,0) when 0 then 100
    else
        ((nvl(a.compare_fine1,0) - a.compare_fine3)*100)/a.compare_fine3
    end
end diff_fine_percent1,
nvl(a.compare_case1 ,0)- nvl(a.compare_case2,0) diff_case2, nvl(a.compare_fine1,0) - nvl(a.compare_fine2,0) diff_fine2,
case when nvl(a.compare_case2,0) = 0 and nvl(a.compare_case1,0)=0 then 0
else
    case nvl(a.compare_case2,0) when 0 then 100
    else
        ((nvl(a.compare_case1,0) - a.compare_case2)*100)/a.compare_case2
    end
end diff_case_percent2,
case when nvl(a.compare_fine2,0) = 0 and nvl(a.compare_fine1,0)=0 then 0
else
    case nvl(a.compare_fine2,0) when 0 then 100
    else
        ((nvl(a.compare_fine1,0) - a.compare_fine2)*100)/a.compare_fine2
    end
end diff_fine_percent2,
nvl((select short_name from ed_office where offcode=nvl($P{getOffcode3},nvl($P{getOffcode2},$P{getOffcode1}))),'รวมทั่วประเทศ') offname,
(select title_name || first_name || ' ' || last_name from v_staff_list_popup where idcard_no=$P{getUserID}) staff_name,
(select month_name_thai from v_month_name where month_no=substr($P{getMonthfrom1},5,2)) month_from,
(select month_name_thai from v_month_name where month_no=substr($P{getMonthto1},5,2)) month_to
from (
    select '0' as GrpKey,lg.legislation_name,'' as grpid,
        (select count(bii.id)
          from book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
        where  substr(aai.offcode,1,2)||'0000'=nvl($P{getOffcode1},substr(aai.offcode,1,2)||'0000')
            and substr(aai.offcode,1,4)||'00'=nvl($P{getOffcode2},substr(aai.offcode,1,4)||'00')
            and aai.offcode=nvl($P{getOffcode3},aai.offcode)
    and to_char(bii.case_date,'yyyymm') between $P{getMonthfrom3} and $P{getMonthto3}
            and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax'))) compare_case3,
          (select sum(nvl(cci.birbe_money,0)+nvl(cci.reward_money,0)+nvl(cci.treasury_money,0))
          from compare_case cci
          inner join book_impeachment bii  on bii.track_no=cci.track_no
          inner join application_arrest aai on aai.track_no=bii.track_no
        where  substr(aai.offcode,1,2)||'0000'=nvl($P{getOffcode1},substr(aai.offcode,1,2)||'0000')
            and substr(aai.offcode,1,4)||'00'=nvl($P{getOffcode2},substr(aai.offcode,1,4)||'00')
            and aai.offcode=nvl($P{getOffcode3},aai.offcode)
        and to_char(cci.pay_date,'yyyymm') between $P{getMonthfrom3} and $P{getMonthto3}
            and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax'))) compare_fine3,
            (select count(bii.id)
          from book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
        where  substr(aai.offcode,1,2)||'0000'=nvl($P{getOffcode1},substr(aai.offcode,1,2)||'0000')
            and substr(aai.offcode,1,4)||'00'=nvl($P{getOffcode2},substr(aai.offcode,1,4)||'00')
            and aai.offcode=nvl($P{getOffcode3},aai.offcode)
            and to_char(bii.case_date,'yyyymm') between $P{getMonthfrom2} and $P{getMonthto2}
            and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax'))) compare_case2,
