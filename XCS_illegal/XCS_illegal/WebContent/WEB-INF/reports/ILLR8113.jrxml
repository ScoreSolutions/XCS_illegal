    left join application_arrest aa on dg.group_id=aa.product_group_id
    where (aa.legislation_id=pke_master.getConfigvalue('legislation_tax') or aa.legislation_id is null)
    and group_status = 'N'
    and substr(dg.group_id,1,1) not in (7,8,9)
