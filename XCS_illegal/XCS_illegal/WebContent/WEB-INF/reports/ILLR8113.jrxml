        where  substr(aai.offcode,1,2)||'0000'=nvl($P{getOffcode1},substr(aai.offcode,1,2)||'0000')
            and substr(aai.offcode,1,4)||'00'=nvl($P{getOffcode2},substr(aai.offcode,1,4)||'00')
            and aai.offcode=nvl($P{getOffcode3},aai.offcode)
        and to_char(cci.pay_date,'yyyymm') between $P{getMonthfrom2} and $P{getMonthto2}
         and aai.legislation_id=aa.legislation_id and aai.product_group_id=aa.product_group_id) compare_case_fine2,
        (select count(bii.id)
           from book_impeachment bii
           inner join application_arrest aai on aai.track_no=bii.track_no
        where  substr(aai.offcode,1,2)||'0000'=nvl($P{getOffcode1},substr(aai.offcode,1,2)||'0000')
            and substr(aai.offcode,1,4)||'00'=nvl($P{getOffcode2},substr(aai.offcode,1,4)||'00')
            and aai.offcode=nvl($P{getOffcode3},aai.offcode)
        and to_char(bii.case_date,'yyyymm') between $P{getMonthfrom1} and $P{getMonthto1}
         and aai.legislation_id=aa.legislation_id and aai.product_group_id=aa.product_group_id) compare_case_qty1,
          (select sum(nvl(cci.birbe_money,0)+nvl(cci.reward_money,0)+nvl(cci.treasury_money,0))
          from compare_case cci
          inner join book_impeachment bii  on bii.track_no=cci.track_no
          inner join application_arrest aai on aai.track_no=bii.track_no
        where  substr(aai.offcode,1,2)||'0000'=nvl($P{getOffcode1},substr(aai.offcode,1,2)||'0000')
            and substr(aai.offcode,1,4)||'00'=nvl($P{getOffcode2},substr(aai.offcode,1,4)||'00')
            and aai.offcode=nvl($P{getOffcode3},aai.offcode)
        and to_char(cci.pay_date,'yyyymm') between $P{getMonthfrom1} and $P{getMonthto1}
         and aai.legislation_id=aa.legislation_id and aai.product_group_id=aa.product_group_id) compare_case_fine1
