	<queryString>
		<![CDATA[select ar.id application_reward_request_id, ar.request_no, ed.offname write_offname, ar.request_date,
to_char(ar.request_date, 'dd/mm/yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  request_date2,
to_char(ar.request_date, 'fmdd', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  date_request_date,
to_char(ar.request_date, 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  month_request_date,
to_char(ar.request_date, 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  year_request_date,
tir.title_name || str.first_name || ' ' || str.last_name request_name, nvl(str.pos_name,psr.posname) ||  str.staff_level posname_request,
pke_arrest.getArrestLawbreaker(aa.track_no) lawbreaker_name,pke_arrest.getArrestIndictment(aa.track_no) guilt_base,
pke_arrest.getsceneaddress(aa.track_no) sceneaddress,
aa.scene_name || ' ' || aa.scene_no scene_no, sd.subdistrict_name scene_subdistrict, d.district_name scene_district, pv.province_name scene_province,
to_char(aa.occurrence_date,'fmdd','NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') occurrence_day,
to_char(aa.occurrence_date,'month','NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') occurrence_month,
to_char(aa.occurrence_date,'yyyy','NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') occurrence_year,
ar.inform_name, nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) case_fine,
nvl(cc.birbe_money,0) birbe_money, nvl(cc.reward_money,0) reward_money,
to_char(cc.pay_date,'fmdd','NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') pay_day,
to_char(cc.pay_date,'month','NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') pay_month,
to_char(cc.pay_date,'yyyy','NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') pay_year,
art.ask_lean, art.department_name_send, art.approve_lean,
edc.offname compare_offname, tic.title_name || stc.first_name || ' ' || stc.last_name staff_name_confirm,
nvl(psc.posname, stc.pos_name) ||stc.staff_level posname_confirm,
tia.title_name || sta.first_name || ' ' || sta.last_name staff_name_ask,
nvl(psa.posname, sta.pos_name) || sta.staff_level posname_ask,
tip.title_name || stp.first_name || ' ' || stp.last_name staff_name_approve,
nvl(psp.posname, stp.pos_name) || stp.staff_level posname_approve,
art.approve_lean
from application_reward_request ar
inner join ed_office ed on ed.offcode=ar.offcode
inner join compare_case cc on cc.id=ar.compare_case_id
inner join application_arrest aa on aa.track_no=cc.track_no
inner join staff str on str.idcard_no=ar.staff_id_request
left join province pv on pv.province_code=aa.scene_province
left join district d on d.district_code=aa.scene_district
left join subdistrict sd on sd.subdistrict_code=aa.scene_subdistrict
left join ed_office edc on edc.offcode=cc.compare_office
left join title tir on tir.title_code=str.title_code
left join position psr on psr.poscode=ar.poscode_request
inner join legislation lg on lg.id=aa.legislation_id
inner join application_reward_req_tax art on ar.id=art.application_reward_request_id
left join staff stc on stc.idcard_no=art.staff_id_confirm
left join position psc on trim(psc.poscode)=trim(art.poscode_confirm)
left join title tic on tic.title_code=stc.title_code
left join staff sta on sta.idcard_no=art.staff_id_ask
left join position psa on psa.poscode=art.poscode_ask
left join title tia on tia.title_code=sta.title_code
left join staff stp on stp.idcard_no=art.staff_id_approve
left join position psp on psp.poscode=art.poscode_approve
left join title tip on tip.title_code=stp.title_code
where ar.request_no = $P{getTrackNo}]]>
