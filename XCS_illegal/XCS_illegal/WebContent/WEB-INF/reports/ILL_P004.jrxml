	<queryString>
		<![CDATA[select distinct si.track_no, ed.offname, si.investigate_no, si.doc_date, ti.title_name || st.first_name || ' ' || st.last_name staff_report_to,psr.posname || st.staff_level posname_info,
lg.legislation_name, decode(si.case_type,'1','รายใหญ่','2','รายย่อย','3','เอเยนต์') case_type,
case s.suspect_type
    when '1' then sti.title_name || s.first_name || ' ' || s.last_name
	when '2' then sti.title_name || s.first_name || ' ' || s.middle_name || ' ' || s.last_name
	when '3' then cti.title_name || s.company_name
end suspect_name, s.other_name, s.suspect_id_card,s.height,
s.birth_date,
    to_char(s.birth_date, 'fmdd', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  dd_birth_date,
    to_char(s.birth_date, 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  mm_birth_date,
    to_char(s.birth_date, 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  yy_birth_date,
to_char(sysdate,'yyyy')-to_char(s.birth_date,'yyyy') age,
decode(s.sex,'0','ชาย','1','หญิง') sex,
decode(s.marital_status,'1','โสด','2','แต่งงาน','3','หม้าย','4','อย่าร้าง') marital_status, s.blood_type,r.race_name,
n.nation_name,trim(re.relation_name || case when tir.title_name is null or tir.title_name ='' then '' else  ' ชื่อ-นามสกุล ' end ||  tir.title_name || sr.first_name || ' ' || sr.last_name) suspect_relation_name,
si.current_address_no,si.current_moo,si.current_address_name,si.current_soi,si.current_road, sdc.subdistrict_name current_subdistrict, dc.district_name current_district, pc.province_name current_province,
si.current_tel, s.address_no,s.moo,s.address_name, s.soi,s.road,sd.subdistrict_name, d.district_name,p.province_name,s.tel,
pke_suspect.getInvestSuspectAddress(si.id)  suspect_address, pke_suspect.getArrestSuspectAddress(s.suspect_id_card) arrest_address,
si.suspect_behavior,pke_suspect.getInvestIndictment(si.id) suspect_indictment,
case s.suspect_type
    when '1' then pke_arrest.getArrestQty(s.suspect_id_card,1)
	when '2' then pke_arrest.getArrestQty(s.suspect_passport,2)
	when '3' then pke_arrest.getArrestQty(s.excise_reg_no,3)
end arrest_qty, si.remarks, tii.title_name || stv.first_name || ' ' || stv.last_name staff_invest, si.investigate_present,
tic.title_name || stc.first_name || ' ' || stc.last_name staff_command, ps.posname || stc.staff_level posname_command,
tif.title_name || stf.first_name || ' ' || stf.last_name staff_info, psf.posname || stf.staff_level posname_info,
tis.title_name || sts.first_name || ' ' || sts.last_name staff_order_save, pss.posname || sts.staff_level posname_order_save,
tik.title_name || stk.first_name || ' ' || stk.last_name staff_key, psk.posname || stk.staff_level posname_key,
    to_char(si.doc_date, 'fmdd', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  dd_docdate,
    to_char(si.doc_date, 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  mm_docdate,
    to_char(si.doc_date, 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  yy_docdate,
    to_char(si.start_date, 'fmdd', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  || ' ' ||
    to_char(si.start_date, 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') || ' ' ||
    to_char(si.start_date, 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  sdate,
    to_char(si.end_date, 'fmdd', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  || ' ' ||
    to_char(si.end_date, 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  || ' ' ||
    to_char(si.end_date, 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  edate,
    to_char(si.start_date, 'hh24:mi') stime,
    to_char(si.end_date, 'hh24:mi') etime,
si.CURRENT_COORDINATE_X  || ', ' ||  si.CURRENT_COORDINATE_Y susxy,
si.SUSPECT_COORDINATE_X  || ', ' ||  si.SUSPECT_COORDINATE_Y cusxy, si.staff_info_mobile sf_mobi,
pke_suspect.getSuspectInvestigateTeam(si.id) staff_name_team,
nvl(psv.posname, stv.pos_name) || stv.staff_level posname_invest
from suspect_investigate si
inner join ed_office ed on ed.offcode=si.offcode
left join staff st on st.idcard_no=si.staff_report_to
left join title ti on ti.title_code=st.title_code
inner join legislation lg on lg.id=si.legislation_id
inner join suspect s on s.suspect_no=si.suspect_no
left join title sti on sti.title_code=s.title_code
left join title cti on cti.title_code=s.title_code_company
left join race r on r.id=s.race_id
left join nationality n on n.id=s.nation_id
left join suspect_relatives sr on sr.suspect_no=s.suspect_no
left join relation re on re.id=sr.relation
left join title tir on tir.title_code=sr.title_code
left join subdistrict sdc on sdc.subdistrict_code=si.current_subdistrict_code
left join district dc on dc.district_code=si.current_district_code
left join province pc on pc.province_code=si.current_province_code
left join subdistrict sd on sd.subdistrict_code=s.subdistrict_code
left join district d on d.district_code=s.district_code
left join province p on p.province_code=s.province_code
left join staff stv on stv.idcard_no=si.staff_investigate
left join title tii on tii.title_code=stv.title_code
left join staff stc on stc.idcard_no=si.staff_command
left join title tic on tic.title_code=stc.title_code
left join position ps on ps.poscode=si.poscode_command
left join staff stf on stf.idcard_no=si.staff_info
left join title tif on tif.title_code=stf.title_code
left join position psf on psf.poscode=si.poscode_info
left join staff sts on sts.idcard_no=si.staff_order_save
left join title tis on tis.title_code=sts.title_code
left join position pss on pss.poscode=si.poscode_order_save
left join staff stk on stk.idcard_no=to_char(si.create_by)
left join title tik on tik.title_code=stk.title_code
left join position psk on trim(psk.poscode)=trim(stk.poscode)
left join position psr on trim(psr.poscode)=trim(st.poscode)
left join suspect_investigate_team sit on sit.suspect_investigate_id=si.id
left join staff stt on stt.idcard_no=sit.idcard_no
left join title tit on tit.title_code=stt.title_code
left join position pst on pst.poscode=sit.poscode
left join position psv on psv.poscode=stv.poscode
where si.track_no=$P{getTrackNo}]]>
	</queryString>
