	<queryString>
		<![CDATA[select ar.id application_reward_request_id, ar.request_no, ed.offname,
to_char(ar.request_date,'fmdd') request_date,
to_char(ar.request_date, 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  request_month,
to_char(ar.request_date, 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  request_year,
tir.title_name || str.first_name || ' ' || str.last_name request_name, psr.posname || str.staff_level posname_request,
tirc.title_name || strc.first_name || ' ' || strc.last_name recieve_name, psrc.posname || strc.staff_level posname_recieve,
pke_arrest.getRewardReqTeam(ar.id,to_number(pke_master.getConfigValue('arrest_co'))) arrest_team,
pke_arrest.getArrestLawbreaker(aa.track_no) lawbreaker_name,inc.guilt_base,
pke_arrest.getRewardReqTeam(ar.id,to_number(pke_master.getConfigValue('commander'))) command_team,
pke_arrest.getSceneAddress(aa.track_no) scene_addr,ara.report_to report_to,
nvl(ara.order_doc_no,'') order_doc_no,
nvl(to_char(ara.order_doc_date,'fmdd') ,'') order_doc_day,
nvl(to_char(ara.order_doc_date,'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI'),'') order_doc_month,
nvl(to_char(ara.order_doc_date,'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI'),'') order_doc_year,
to_char(aa.occurrence_date,'fmdd') occurrence_date,
to_char(aa.occurrence_date,'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') occurrence_month,
to_char(aa.occurrence_date,'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') occurrence_year,
to_char(ar.receive_date,'fmdd') recieve_date,
to_char(ar.receive_date,'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') recieve_month,
to_char(ar.receive_date,'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') recieve_year,
ar.inform_name,
nvl(cc.birbe_money,0)+nvl(cc.reward_money,0)+nvl(cc.treasury_money,0) case_fine,
nvl(cc.birbe_money,0) birbe_money, nvl(cc.reward_money,0) reward_money, cc.compare_case_date,
nvl(aa.accuser_name,tia.title_name || sta.first_name || ' ' || sta.last_name) accuser_name, nvl(aa.accuser_posname, psa.posname) || sta.staff_level accuser_posname,
nvl(aa.accuser_offname,eda.offname) accuser_offname,
tip.title_name || stp.first_name || ' ' || stp.last_name staff_prove, nvl(stp.pos_name,psp.posname) || stp.staff_level posname_prove,
tic.title_name || stc.first_name || ' ' || stc.last_name staff_sue, nvl(stc.pos_name,psc.posname) || stc.staff_level posname_sue,
ara.accept_lean, ara.accept_desc,lg.legislation_name,ara.leader_lean,
ara.leader_accept, ara.leader_accept_reason, ara.leader_deny_reason, til.title_name || stl.first_name || ' ' || stl.last_name staff_leader,
pol.posname || ' ' || stl.staff_level posname_leader,
ara.section_director_lean, ara.section_director_accept, ara.section_director_accept_reason, ara.section_director_deny_reason,
tis.title_name || sts.first_name || ' ' || sts.last_name staff_director, pss.posname || sts.staff_level posname_director,
ara.res_director_accept, ara.res_director_accept_reason, ara.res_director_deny_reason,
tie.title_name || ste.first_name || ' ' || ste.last_name staff_res_director, pse.posname || ste.staff_level posname_res_director
from application_reward_request ar
inner join ed_office ed on ed.offcode=ar.offcode
inner join compare_case cc on cc.id=ar.compare_case_id
left join indictment inc on cc.indictment_id=inc.indictment_id
inner join application_arrest aa on aa.track_no=cc.track_no
left join staff str on str.idcard_no=ar.staff_id_request
left join title tir on tir.title_code=str.title_code
left join position psr on psr.poscode=trim(ar.poscode_request)
left join staff strc on strc.idcard_no=ar.staff_id_receive
left join title tirc on tirc.title_code=strc.title_code
left join position psrc on psrc.poscode=trim(ar.poscode_receive)
left join staff sta on sta.idcard_no=aa.accuser_idcardno
left join title tia on tia.title_code=sta.title_code
left join position psa on psa.poscode=aa.accuser_poscode
left join ed_office eda on eda.offcode=aa.accuser_offcode
left join application_arrest_prove aap on aap.track_no=aa.track_no
left join staff stp on stp.idcard_no=aap.staff_idcardno_prove
left join title tip on tip.title_code=stp.title_code
left join position psp on psp.poscode=aap.staff_prove_poscode
left join staff stc on stc.idcard_no=cc.sue_staff_idcardno
left join title tic on tic.title_code=stc.title_code
left join position psc on trim(cc.sue_poscode)=psc.poscode
inner join legislation lg on lg.id=aa.legislation_id
inner join application_reward_req_alcohol ara on ar.id=ara.application_reward_request_id
left join staff stl on stl.idcard_no=ara.staff_id_leader
left join title til on til.title_code=stl.title_code
left join position pol on pol.poscode=ara.poscode_leader
left join staff sts on sts.idcard_no=ara.staff_id_section_director
left join title tis on tis.title_code=sts.title_code
left join position pss on pss.poscode=ara.poscode_section_director
left join staff ste on ste.idcard_no=ara.staff_id_res_director
left join title tie on tie.title_code=ste.title_code
left join position pse on pse.poscode=ara.poscode_res_director
where ar.request_no = $P{getTrackNo}]]>
