	<queryString>
		<![CDATA[select a.*,a.case_qty+a.court_qty+a.noculprit_qty as total_qty,
a.case_exhibit+a.court_exhibit+a.noculprit_exhibit total_exhibit,
a.case_fine+a.court_fine+a.noculprit_fine total_fine,
nvl((select short_name from ed_office where offcode = nvl($P{vOffcode3},nvl($P{vOffcode2},$P{vOffcode1}))),'รวมทั่วประเทศ') offname,
to_char(sysdate, 'fmdd month yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') untildate,
to_char(sysdate, 'hh24:mi', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') systimedate,
     (select ti.title_name || st.first_name || ' ' || st.last_name staff_name
                          from staff st
                          left join title ti on ti.title_code=st.title_code
                          where st.idcard_no=$P{getUserID}) user_name,
to_char(to_date($P{vDateFrom},'yyyy-mm-dd'), 'fmdd', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  date_from,
to_char(to_date($P{vDateFrom},'yyyy-mm-dd'), 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  month_from,
to_char(to_date($P{vDateFrom},'yyyy-mm-dd'), 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  year_from
from (
    select  lg.id legislation_id,  lg.legislation_name, nvl(aae.product_name,dt.duty_name) product_name,
    nvl(aae.netweight_unit_name, wu.thname) netweignt_unit_name,
        --คดีเปรียบเทียบปรับ
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'1','1') case_qty,
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'1','2') case_exhibit,
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'1','3') case_fine,
        --คดีฟ้องศาล
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'2','1') court_qty,
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'2','2') court_exhibit,
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'2','3') court_fine,
        --คดีไม่มีตัว
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'3','1') noculprit_qty,
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'3','2') noculprit_exhibit,
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'3','3') noculprit_fine,
        --คดีใบอนุญาติ
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'4','1') license_qty,
        pke_report2.getCase2_13($P{vDateFrom},$P{vDateTo},$P{vOffcode1},$P{vOffcode2},$P{vOffcode3},lg.id,nvl(aae.product_name,dt.duty_name),nvl(aae.netweight_unit_name, wu.thname),'4','3') license_fine
    from application_arrest aa
    inner join application_arrest_exhibit aae on aa.track_no=aae.track_no
    inner join legislation lg on lg.id=aa.legislation_id
    inner join book_impeachment bi on bi.track_no=aa.track_no
    left join duty_table dt on dt.duty_code=aae.duty_code
    left join unit wu on wu.unit_code=aae.netweight_unit_code
    where to_char(bi.case_date,'yyyy-mm-dd') between $P{vDateFrom} and $P{vDateTo}
    and substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
    and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
    and aa.offcode=nvl($P{vOffcode3},aa.offcode)
    group by  lg.id,  lg.legislation_name, aae.product_name,dt.duty_name , aae.netweight_unit_name, wu.thname
    order by lg.id, nvl(aae.product_name,dt.duty_name)
) a
]]>
