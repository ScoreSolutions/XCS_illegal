	<queryString>
		<![CDATA[select mn.month_name_thai, pke_reports.getCaseByOffcode(nvl($P{vOffcode3},nvl($P{vOffcode2},$P{vOffcode1})),mn.month_no, $P{vYear},pke_master.getConfigValue('legislation_alcohol'),$P{rtType},1) case_qty,
nvl((select short_name from ed_office where offcode = nvl($P{vOffcode3},nvl($P{vOffcode2},$P{vOffcode1}))),'รวมทั่วประเทศ') offname,
(select title_name || first_name || ' ' || last_name from v_staff_list_popup where idcard_no=$P{getUserID}) staff_name,
to_char(sysdate,'fmdd month yyyy HH24:mi', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') date_Test,

 --จำนวนของกลางจากคดีเปรียบเทียบปรับ
    (select sum(nvl(aae.netweight,0)) from application_arrest_exhibit aae inner join application_arrest aa on aa.track_no=aae.track_no
    inner join book_impeachment bi on bi.track_no=aae.track_no
    where  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
    and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
    and aa.offcode=nvl($P{vOffcode3},aa.offcode)
    and to_char(bi.case_date,'yyyymmdd') between ($P{vYear} -mn.isYear)||mn.month_no||'01' and ($P{vYear}-mn.isYear)||mn.month_no||pke_reports.getLastDay(($P{vYear} -mn.isYear),mn.month_no)
    and aa.legislation_id=pke_master.getConfigValue('legislation_alcohol')
    and aae.netweight_unit_code ='01' and (aae.duty_code='7002' or aae.product_name like '%กลั่น%')) exhibit_distill,
    (select sum(nvl(aae.netweight,0)) from application_arrest_exhibit aae inner join application_arrest aa on aa.track_no=aae.track_no
    inner join book_impeachment bi on bi.track_no=aae.track_no
    where  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
    and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
    and aa.offcode=nvl($P{vOffcode3},aa.offcode)
    and to_char(bi.case_date,'yyyymmdd') between ($P{vYear} -mn.isYear)||mn.month_no||'01' and ($P{vYear}-mn.isYear)||mn.month_no||pke_reports.getLastDay(($P{vYear} -mn.isYear),mn.month_no)
    and aa.legislation_id=pke_master.getConfigValue('legislation_alcohol')
    and aae.netweight_unit_code ='01' and (aae.duty_code='7001' or aae.product_name like '%แช่%')) exhibit_dip,
    pke_reports.getCaseByOffcode(nvl($P{vOffcode1},nvl($P{vOffcode2},$P{vOffcode3})),mn.month_no, $P{vYear},pke_master.getConfigValue('legislation_alcohol'), $P{rtType},3) case_fine,
    pke_reports.getCaseNoCulpritQty(nvl($P{vOffcode1},nvl($P{vOffcode2},$P{vOffcode3})),to_date(($P{vYear} -mn.isyear)||'-'||mn.month_no||'-01','yyyy-mm-dd'),to_date(($P{vYear} -mn.isyear)||'-'|| mn.month_no ||'-'||pke_reports.getLastDay(($P{vYear} -mn.isyear),mn.month_no),'yyyy-mm-dd'),pke_master.getConfigValue('legislation_alcohol'),$P{rtType}) noculprit_qty,
    --จำนวนของกลางจากคดีไม่มีตัว
    (select sum(nvl(aae.netweight,0)) from application_arrest_exhibit aae inner join application_arrest aa on aa.track_no=aae.track_no
    inner join book_impeachment bi on bi.track_no=aae.track_no
    where  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
    and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
    and aa.offcode=nvl($P{vOffcode3},aa.offcode)
    and to_char(bi.case_date,'yyyymmdd') between ($P{vYear} -mn.isYear)||mn.month_no||'01' and ($P{vYear}-mn.isYear)||mn.month_no||pke_reports.getLastDay(($P{vYear} -mn.isYear),mn.month_no)
    and aa.legislation_id=pke_master.getConfigValue('legislation_alcohol') and aa.have_culprit='N'
    and aae.netweight_unit_code ='01' and (aae.duty_code='7002' or aae.product_name like '%กลั่น%')) noculprit_exhibit_distill,
    (select sum(nvl(aae.netweight,0)) from application_arrest_exhibit aae inner join application_arrest aa on aa.track_no=aae.track_no
    inner join book_impeachment bi on bi.track_no=aae.track_no
    where  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
    and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
    and aa.offcode=nvl($P{vOffcode3},aa.offcode)
    and to_char(bi.case_date,'yyyymmdd') between ($P{vYear} -mn.isYear)||mn.month_no||'01' and ($P{vYear}-mn.isYear)||mn.month_no||pke_reports.getLastDay(($P{vYear} -mn.isYear),mn.month_no)
    and aa.legislation_id=pke_master.getConfigValue('legislation_alcohol')  and aa.have_culprit='N'
    and aae.netweight_unit_code ='01' and (aae.duty_code='7001' or aae.product_name like '%แช่%')) noculprit_exhibit_dip,
    pke_reports.getCaseNoCulpritFine(nvl($P{vOffcode1},nvl($P{vOffcode2},$P{vOffcode3})),to_date(($P{vYear}-mn.isyear)||'-'||mn.month_no||'-01','yyyy-mm-dd'),to_date(($P{vYear} -mn.isyear)||'-'|| mn.month_no ||'-'||pke_reports.getLastDay(($P{vYear} -mn.isyear),mn.month_no),'yyyy-mm-dd'),pke_master.getConfigValue('legislation_alcohol'),$P{rtType}) noculprit_fine,
    pke_reports.getSendCourtQty(nvl($P{vOffcode1},nvl($P{vOffcode2},$P{vOffcode3})),to_date(($P{vYear} -mn.isyear)||'-'||mn.month_no||'-01','yyyy-mm-dd'),to_date(($P{vYear} -mn.isyear)||'-'|| mn.month_no ||'-'||pke_reports.getLastDay(($P{vYear} -mn.isyear),mn.month_no),'yyyy-mm-dd'),pke_master.getConfigValue('legislation_alcohol'),'2',$P{rtType}) sendcourt_qty,
    --จำนวนของกลางจากคดีฟ้องศาล
    (select sum(nvl(aae.netweight,0)) from application_arrest_exhibit aae inner join application_arrest aa on aa.track_no=aae.track_no
    inner join book_impeachment bi on bi.track_no=aae.track_no
    where  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
    and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
    and aa.offcode=nvl($P{vOffcode3},aa.offcode)
    and to_char(bi.case_last_date,'yyyymmdd') between ($P{vYear} -mn.isYear)||mn.month_no||'01' and ($P{vYear}-mn.isYear)||mn.month_no||pke_reports.getLastDay(($P{vYear} -mn.isYear),mn.month_no)
    and aa.legislation_id=pke_master.getConfigValue('legislation_alcohol') and bi.case_quality='2'
    and aae.netweight_unit_code ='01' and (aae.duty_code='7002' or aae.product_name like '%กลั่น%')) sendcourt_exhibit_distill,
    (select sum(nvl(aae.netweight,0)) from application_arrest_exhibit aae inner join application_arrest aa on aa.track_no=aae.track_no
    inner join book_impeachment bi on bi.track_no=aae.track_no
    where  substr(aa.offcode,1,2)||'0000'=nvl($P{vOffcode1},substr(aa.offcode,1,2)||'0000')
    and substr(aa.offcode,1,4)||'00'=nvl($P{vOffcode2},substr(aa.offcode,1,4)||'00')
    and aa.offcode=nvl($P{vOffcode3},aa.offcode)
    and to_char(bi.case_last_date,'yyyymmdd') between ($P{vYear} -mn.isYear)||mn.month_no||'01' and ($P{vYear}-mn.isYear)||mn.month_no||pke_reports.getLastDay(($P{vYear} -mn.isYear),mn.month_no)
    and aa.legislation_id=pke_master.getConfigValue('legislation_alcohol')  and bi.case_quality='2'
    and aae.netweight_unit_code ='01' and (aae.duty_code='7001' or aae.product_name like '%แช่%')) sendcourt_exhibit_dip,
    pke_reports.getSendCourtSUM(nvl($P{vOffcode1},nvl($P{vOffcode2},$P{vOffcode3})),to_date(($P{vYear} -mn.isyear)||'-'||mn.month_no||'-01','yyyy-mm-dd'),to_date(($P{vYear} -mn.isyear)||'-'||mn.month_no||'-'||pke_reports.getLastDay(($P{vYear} -mn.isyear),mn.month_no),'yyyy-mm-dd'),pke_master.getConfigValue('legislation_alcohol'),'2',$P{rtType}) sendcourt_fine
from v_month_name mn
order by mn.budget_year_no]]>
	</queryString>
