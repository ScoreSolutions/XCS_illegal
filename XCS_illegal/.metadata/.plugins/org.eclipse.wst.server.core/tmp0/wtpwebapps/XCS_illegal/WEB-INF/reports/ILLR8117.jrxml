     pke_reports.getCase1_16(ed.offcode,pke_master.getConfigValue('legislation_tobacco'), $P{vYear},$P{vMonthFrom},$P{vMonthTo},'2','3') noculprit_fine,
     0 desc_case, 0 desc_fine,
     pke_reports.getCase1_16(ed.offcode,pke_master.getConfigValue('legislation_tobacco'), $P{vYear},$P{vMonthFrom},$P{vMonthTo},'','','ยาสูบต่างประเทศ') tobacco_outbound,
     pke_reports.getCase1_16(ed.offcode,pke_master.getConfigValue('legislation_tobacco'), $P{vYear},$P{vMonthFrom},$P{vMonthTo},'','กรองทิพย์') krongtip,
