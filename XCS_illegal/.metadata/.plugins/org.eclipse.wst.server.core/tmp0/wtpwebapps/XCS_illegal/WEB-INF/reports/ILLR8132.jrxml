	<queryString>
		<![CDATA[select a.legislation_name, a.grpid, a.pgroup,
a.case_qty1, a.case_fine1, a.court_qty1, a.court_fine1, a.noculprit_qty1, a.noculprit_fine1,
nvl(a.case_qty1,0)+nvl(a.court_qty1,0)+nvl(a.noculprit_qty1,0) all_case1_qty,
nvl(a.case_fine1,0)+nvl(a.court_fine1,0)+nvl(a.noculprit_fine1,0) all_case1_fine,
a.case_qty2, a.case_fine2, a.court_qty2, a.court_fine2, a.noculprit_qty2, a.noculprit_fine2,
nvl(a.case_qty2,0)+nvl(a.court_qty2,0)+nvl(a.noculprit_qty2,0) all_case2_qty,
nvl(a.case_fine2,0)+nvl(a.court_fine2,0)+nvl(a.noculprit_fine2,0) all_case2_fine,
nvl(a.case_qty1,0)+nvl(a.court_qty1,0)+nvl(a.noculprit_qty1,0)+nvl(a.case_qty2,0)+nvl(a.court_qty2,0)+nvl(a.noculprit_qty2,0) total_qty,
nvl(a.case_fine1,0)+nvl(a.court_fine1,0)+nvl(a.noculprit_fine1,0)+nvl(a.case_fine2,0)+nvl(a.court_fine2,0)+nvl(a.noculprit_fine2,0) total_fine,
  to_char(sysdate, 'fmdd month yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') untildate,
to_char(sysdate, 'hh24:mi', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') systimedate,
        (select ti.title_name || st.first_name || ' ' || st.last_name staff_name
                          from staff st
                          left join title ti on ti.title_code=st.title_code
                          where st.idcard_no=$P{getUserID}) user_name,
to_char(to_date(($P{vYear}-543)||'-'||substr($P{vMonth},1,2)||'-01','yyyy-mm-dd'), 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') as MONTH_NAME ,
to_char(to_date(substr($P{vMonth},3,5)||'-'||substr($P{vMonth},1,2)||'-01','yyyy-mm-dd'), 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') as year_name
from (
  select lg.legislation_name,'' as grpid, 1 pgroup,
         --ส่วนกลาง
        (select count(bii.id)
        from book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2)='10'
        and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax'))) case_qty1,
        (select sum(nvl(cci.birbe_money,0)+nvl(cci.reward_money,0)+nvl(cci.treasury_money,0))
        from compare_case cci
        inner join book_impeachment bii  on bii.track_no=cci.track_no
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2)='10'
        and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax'))) case_fine1,
        (select count(bii.id)
        from book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2)='10'
        and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax')) and bii.case_quality<>'1') court_qty1,
        (select sum(nvl(bii.case_fine,0))
        from  book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2)='10'
        and to_char(bii.case_last_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax'))) court_fine1,
        (select count(bii.id)
        from book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2)='10'
        and to_char(aai.application_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax')) and aai.have_culprit='N') noculprit_qty1,
        (select sum(nvl(aai.estimate_fine,0))
        from  book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2)='10'
        and to_char(aai.application_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax')) and aai.have_culprit='N') noculprit_fine1,
        ---ส่วนภูมิภาค
        (select count(bii.id)
        from book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2)  between '01' and '09'
        and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax'))) case_qty2,
        (select sum(nvl(cci.birbe_money,0)+nvl(cci.reward_money,0)+nvl(cci.treasury_money,0))
        from compare_case cci
        inner join book_impeachment bii  on bii.track_no=cci.track_no
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2) between '01' and '09'
        and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax'))) case_fine2,
        (select count(bii.id)
        from book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2) between '01' and '09'
        and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax')) and bii.case_quality<>'1') court_qty2,
        (select sum(nvl(bii.case_fine,0))
        from  book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2) between '01' and '09'
        and to_char(bii.case_last_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax')) and bii.case_quality<>'1') court_fine2,
        (select count(bii.id)
        from book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2) between '01' and '09'
        and to_char(aai.application_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax')) and aai.have_culprit='N') noculprit_qty2,
        (select sum(nvl(aai.estimate_fine,0))
        from  book_impeachment bii
        inner join application_arrest aai on aai.track_no=bii.track_no
        where substr(aai.offcode,1,2) between '01' and '09'
        and to_char(aai.application_date,'mmyyyy') = $P{vMonth}
        and aai.legislation_id=lg.id and aai.legislation_id not in (pke_master.getConfigvalue('legislation_tax')) and aai.have_culprit='N') noculprit_fine2
    from legislation lg
    union all
        select aa.group_name,aa.grpid,aa.pgroup,aa.case_qty1,aa.case_fine1,
    aa.court_qty1,aa.court_fine1,aa.noculprit_qty1,aa.noculprit_fine1,
    aa.case_qty2,aa.case_fine2,aa.court_qty2,aa.court_fine2,aa.noculprit_qty2,aa.noculprit_fine2
    from (
    select '    - ' || dg.group_name as group_name ,dg.group_id as grpid, 2 pgroup,
           --ส่วนกลาง
           (select count(bii.id)
           from book_impeachment bii
           inner join application_arrest aai on aai.track_no=bii.track_no
           where  substr(aai.offcode,1,2)='10'
           and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
           and  aai.product_group_id=dg.group_id) case_qty1,
          (select sum(nvl(cci.birbe_money,0)+nvl(cci.reward_money,0)+nvl(cci.treasury_money,0))
          from compare_case cci
          inner join book_impeachment bii  on bii.track_no=cci.track_no
          inner join application_arrest aai on aai.track_no=bii.track_no
          where  substr(aai.offcode,1,2)='10'
          and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
          and  aai.product_group_id=dg.group_id) case_fine1,
          (select count(bii.id)
          from book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
          where substr(aai.offcode,1,2)='10'
          and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
          and aai.product_group_id=dg.group_id and bii.case_quality<>'1') court_qty1,
          (select sum(nvl(bii.case_fine,0))
          from  book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
          where substr(aai.offcode,1,2)='10'
          and to_char(bii.case_last_date,'mmyyyy') = $P{vMonth}
          and aai.product_group_id=dg.group_id and bii.case_quality<>'1') court_fine1,
          (select count(bii.id)
          from book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
          where substr(aai.offcode,1,2)='10'
          and to_char(aai.application_date,'mmyyyy') = $P{vMonth}
          and aai.product_group_id=dg.group_id  and aai.have_culprit='N') noculprit_qty1,
          (select sum(nvl(aai.estimate_fine,0))
          from  book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
          where substr(aai.offcode,1,2)='10'
          and to_char(aai.application_date,'mmyyyy') = $P{vMonth}
          and aai.product_group_id=dg.group_id and aai.have_culprit='N') noculprit_fine1,
          --ส่วนภูมิภาค
          (select count(bii.id)
           from book_impeachment bii
           inner join application_arrest aai on aai.track_no=bii.track_no
           where  substr(aai.offcode,1,2) between '01' and '09'
           and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
           and  aai.product_group_id=dg.group_id) case_qty2,
          (select sum(nvl(cci.birbe_money,0)+nvl(cci.reward_money,0)+nvl(cci.treasury_money,0))
          from compare_case cci
          inner join book_impeachment bii  on bii.track_no=cci.track_no
          inner join application_arrest aai on aai.track_no=bii.track_no
          where  substr(aai.offcode,1,2) between '01' and '09'
          and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
          and  aai.product_group_id=dg.group_id) case_fine2,
          (select count(bii.id)
          from book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
          where substr(aai.offcode,1,2) between '01' and '09'
          and to_char(bii.case_date,'mmyyyy') = $P{vMonth}
          and aai.product_group_id=dg.group_id and bii.case_quality<>'1') court_qty2,
          (select sum(nvl(bii.case_fine,0))
          from  book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
          where substr(aai.offcode,1,2) between '01' and '09'
          and to_char(bii.case_last_date,'mmyyyy') = $P{vMonth}
          and aai.product_group_id=dg.group_id and bii.case_quality<>'1') court_fine2,
          (select count(bii.id)
          from book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
          where substr(aai.offcode,1,2) between '01' and '09'
          and to_char(aai.application_date,'mmyyyy') = $P{vMonth}
          and aai.product_group_id=dg.group_id  and aai.have_culprit='N') noculprit_qty2,
          (select sum(nvl(aai.estimate_fine,0))
          from  book_impeachment bii
          inner join application_arrest aai on aai.track_no=bii.track_no
          where substr(aai.offcode,1,2) between '01' and '09'
          and to_char(aai.application_date,'mmyyyy') = $P{vMonth}
          and aai.product_group_id=dg.group_id and aai.have_culprit='N') noculprit_fine2
    from duty_group dg
    where substr(dg.group_id,1,1) not in (7,8,9) and dg.group_status='N'
   order by grpid
     ) aa
) a]]>
