	<queryString>
		<![CDATA[select ed.offcode, ed.short_name,
pke_reports.getCaseQty(ed.offcode,to_date($P{vDateFrom},'yyyy-mm-dd'),to_date($P{vDateTo},'yyyy-mm-dd'),pke_master.getConfigValue('legislation_tobacco'),'2') case_qty,
pke_reports.getCompareCaseSum(ed.offcode, to_date($P{vDateFrom},'yyyy-mm-dd'),to_date($P{vDateTo},'yyyy-mm-dd'),pke_master.getConfigValue('legislation_tobacco'),'2') case_fine,
pke_reports.getSendCourtQty(ed.offcode, to_date($P{vDateFrom},'yyyy-mm-dd'),to_date($P{vDateTo},'yyyy-mm-dd'),pke_master.getConfigValue('legislation_tobacco'),'2','2') court_qty,
pke_reports.getSendCourtSum(ed.offcode, to_date($P{vDateFrom},'yyyy-mm-dd'),to_date($P{vDateTo},'yyyy-mm-dd'),pke_master.getConfigValue('legislation_tobacco'),'2','2') court_fine,
pke_reports.getCaseNoCulpritQty(ed.offcode, to_date($P{vDateFrom},'yyyy-mm-dd'),to_date($P{vDateTo},'yyyy-mm-dd'),pke_master.getConfigValue('legislation_tobacco'),'2') noculprit_qty,
pke_reports.getCaseNoCulpritFine(ed.offcode, to_date($P{vDateFrom},'yyyy-mm-dd'),to_date($P{vDateTo},'yyyy-mm-dd'),pke_master.getConfigValue('legislation_tobacco'),'2') noculprit_fine,
  nvl((select sum(nvl(aae.qty,0))
  from application_arrest_exhibit aae
  inner join application_arrest aa on aa.track_no=aae.track_no
  inner join book_impeachment bi on bi.track_no=aa.track_no
  left join duty_table dt on dt.duty_code=aae.duty_code
  left join unit qu on qu.unit_code=aae.qty_unit_code
  where substr(aa.offcode,1,2)||'0000'=ed.offcode and aa.legislation_id=pke_master.getConfigValue('legislation_tobacco')
  and to_char(bi.case_date,'yyyy-mm-dd') between $P{vDateFrom} and $P{vDateTo}
  and nvl(aae.product_name,dt.duty_name) like '%ต่างประเทศ%'
  and nvl(aae.qty_unit_name, qu.thname) like '%ซอง%'),0) outbound,
  nvl((select sum(nvl(aae.qty,0))
  from application_arrest_exhibit aae
  inner join application_arrest aa on aa.track_no=aae.track_no
  inner join book_impeachment bi on bi.track_no=aa.track_no
  left join brand_main bm on bm.group_id=aae.group_id and bm.brand_main_code=aae.brand_code
  left join unit qu on qu.unit_code=aae.qty_unit_code
  where substr(aa.offcode,1,2)||'0000'=ed.offcode and aa.legislation_id=pke_master.getConfigValue('legislation_tobacco')
  and to_char(bi.case_date,'yyyy-mm-dd') between $P{vDateFrom} and $P{vDateTo}
  and nvl(aae.brand_name,bm.brand_main_thai) like '%กรองทิพย์ 90%'
  and nvl(aae.qty_unit_name, qu.thname) like '%ซอง%'),0) krongtip,
  nvl((select sum(nvl(aae.qty,0))
  from application_arrest_exhibit aae
  inner join application_arrest aa on aa.track_no=aae.track_no
  inner join book_impeachment bi on bi.track_no=aa.track_no
  left join brand_main bm on bm.group_id=aae.group_id and bm.brand_main_code=aae.brand_code
  left join unit qu on qu.unit_code=aae.qty_unit_code
  where substr(aa.offcode,1,2)||'0000'=ed.offcode and aa.legislation_id=pke_master.getConfigValue('legislation_tobacco')
  and to_char(bi.case_date,'yyyy-mm-dd') between $P{vDateFrom} and $P{vDateTo}
  and nvl(aae.brand_name,bm.brand_main_thai) like '%สายฝน%'
  and nvl(aae.qty_unit_name, qu.thname) like '%ซอง%'),0) saifon,
  nvl((select sum(nvl(aae.qty,0))
  from application_arrest_exhibit aae
  inner join application_arrest aa on aa.track_no=aae.track_no
  inner join book_impeachment bi on bi.track_no=aa.track_no
  left join brand_main bm on bm.group_id=aae.group_id and bm.brand_main_code=aae.brand_code
  left join unit qu on qu.unit_code=aae.qty_unit_code
  where substr(aa.offcode,1,2)||'0000'=ed.offcode and aa.legislation_id=pke_master.getConfigValue('legislation_tobacco')
  and to_char(bi.case_date,'yyyy-mm-dd') between $P{vDateFrom} and $P{vDateTo}
  and nvl(aae.brand_name,bm.brand_main_thai) not like '%กรองทิพย์ 90%'
  and nvl(aae.brand_name,bm.brand_main_thai) not like '%สายฝน%'
  and nvl(aae.qty_unit_name, qu.thname) like '%ซอง%'),0) other,
  nvl((select sum(nvl(aae.netweight,0))
  from application_arrest_exhibit aae
  inner join application_arrest aa on aa.track_no=aae.track_no
  inner join book_impeachment bi on bi.track_no=aa.track_no
  left join duty_table dt on dt.duty_code=aae.duty_code
  left join unit qu on qu.unit_code=aae.netweight_unit_code
  where substr(aa.offcode,1,2)||'0000'=ed.offcode and aa.legislation_id=pke_master.getConfigValue('legislation_tobacco')
  and to_char(bi.case_date,'yyyy-mm-dd') between $P{vDateFrom} and $P{vDateTo}
  and (nvl(aae.product_name,dt.duty_name) like '%ยาเส้น%'
  or nvl(aae.product_name,dt.duty_name) like '%ใบยา%')
  and nvl(aae.netweight_unit_name, qu.thname) like '%กรัม%'),0) leaf,
to_char(sysdate, 'fmdd month yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') untildate,
to_char(sysdate, 'hh24:mi', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI') systimedate,
to_char(to_date($P{vDateFrom},'yyyy-mm-dd'), 'fmdd', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  date_from,
to_char(to_date($P{vDateFrom},'yyyy-mm-dd'), 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  month_from,
to_char(to_date($P{vDateFrom},'yyyy-mm-dd'), 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  year_from,
to_char(to_date($P{vDateTo},'yyyy-mm-dd'), 'fmdd', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  date_to,
to_char(to_date($P{vDateTo},'yyyy-mm-dd'), 'month', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  month_to,
to_char(to_date($P{vDateTo},'yyyy-mm-dd'), 'yyyy', 'NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI')  year_to,
        (select ti.title_name || st.first_name || ' ' || st.last_name staff_name
                          from staff st
                          left join title ti on ti.title_code=st.title_code
                          where st.idcard_no=$P{getUserID}) user_name
from ed_office ed
where substr(ed.offcode,3,4)='0000'
order by ed.offcode]]>
	</queryString>
