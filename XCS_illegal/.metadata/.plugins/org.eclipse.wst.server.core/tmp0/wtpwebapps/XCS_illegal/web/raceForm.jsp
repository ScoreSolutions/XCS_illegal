		<td width="172"><strong>CREATE_BY</strong></td>
		<td>
					<input type="text" name="create_by" value="${race.create_by}"/>
				</td>
	</tr>
	<tr>
		<td width="172"><strong>RACE_NAME</strong></td>
		<td>
					<input type="text" name="race_name" value="${race.race_name}"/>
				</td>
	</tr>
	<tr>
		<td width="172"><strong>RACE_CODE</strong></td>
		<td>
					<input type="text" name="race_code" value="${race.race_code}"/>
				</td>
	</tr>
